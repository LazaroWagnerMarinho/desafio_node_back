<!DOCTYPE html>
<html>
<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>Usuarios</title>
</head>
<body>
    <h1>Usuarios</h1>
    <table>
        <tr>
            <td>ID</td>
            <td>Usuario</td>
            <td>Email</td>
            <td>Hospedagem</td>
        </tr>

        <% for(var i=0; i< usuario.length; i++) { %>
            <tr>
                <td><%= usuario[i].id %></td>
                <td><%= usuario[i].usuario %></td>
                <td><%= usuario[i].email %></td>
                <td><%= usuario[i].hospedagem %></td>
            </tr>
        <% } %>
    </table>    
</body>
</html>